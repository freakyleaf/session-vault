{
  "name": "studio-log",
  "license": "Apache-2.0",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "api:build": "cd packages/api && pnpm run build",
    "api:dev": "cd packages/api && pnpm run dev",
    "backend:build": "cd packages/backend && pnpm run build",
    "backend:dev": "cd packages/backend && pnpm run dev",
    "dev": "concurrently --names \"API_____,BACKEND_,FRONTEND\" -c \"cyan,magenta,yellow\" --kill-others-on-fail \"pnpm api:dev\" \"pnpm backend:dev\" \"pnpm frontend:dev\"",
    "frontend:build": "cd packages/frontend && pnpm run build",
    "frontend:dev": "cd packages/frontend && pnpm run dev",
    "lint:scripts": "eslint",
    "lint:styles": "stylelint \"**/*.scss\"",
    "prepare": "husky"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.32.4",
    "@clerk/express": "^1.7.4",
    "@clerk/react-router": "^1.6.4",
    "@hookform/resolvers": "^5.1.1",
    "@reduxjs/toolkit": "^2.8.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "mongoose": "^8.16.1",
    "primeflex": "^3.0.0",
    "primeicons": "^7.0.0",
    "primereact": "^10.9.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.60.0",
    "react-redux": "^9.2.0",
    "react-router": "^7.6.3",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.5.2",
    "concurrently": "^9.2.0",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "sass-embedded": "^1.89.2",
    "stylelint": "^16.21.1",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "unplugin-auto-import": "^19.3.0",
    "vite": "^7.0.0"
  }
}
